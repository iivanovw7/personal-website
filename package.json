{
    "name": "personal-website",
    "version": "1.0.0",
    "description": "personal website",
    "main": "app.js",
    "scripts": {
        "build": "run-s \"clean\" \"build:webpack -- {@}\" \"build:gulp:postbuild -- {@}\" --",
        "build:gulp:postbuild": "gulp postbuild",
        "build:webpack": "node ./tool/build.js",
        "coverage": "npm test -- --coverage",
        "clean": "rm -rf dist",
        "dev": "run-s \"dev:server -- {@}\" --",
        "dev:dashboard": "run-s \"dev:server:dashboard -- {@}\" --",
        "dev:server": "webpack serve --mode='development' --config='./config/webpack/webpack.dev.js' --watch",
        "dev:server:dashboard": "webpack-dashboard -- webpack serve --mode='development' --config='./config/webpack/webpack.dev.js' --watch --open='firefox'",
        "doc": "jsdoc -c config/jsdoc/conf.json",
        "generate": "graphql-codegen",
        "lint": "run-s \"lint:js\" \"lint:css\"",
        "lint:css": "stylelint ./src/**/*.{js,jsx}",
        "lint:js": "npm run lint:eslint",
        "lint:js:fix": "npm run lint:eslint:fix",
        "lint:eslint": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx --ignore-pattern internals/scripts --ignore-path src/.eslintignore",
        "lint:eslint:fix": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx --ignore-pattern internals/scripts --ignore-path src/.eslintignore --fix",
        "pre-commit": "run-s \"clean\" \"lint\"",
        "pre-push": "run-s \"clean\" \"lint\"",
        "reset": "rm -rf dist && rm -rf node_modules && rm package-lock.json",
        "test": "jest --passWithNoTests --verbose --colors --config jest.config.js",
        "view-build": "node tool/view.js"
    },
    "author": "iivanovw7",
    "license": "ISC",
